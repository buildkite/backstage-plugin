{
  "name": "@buildkite/backstage-plugin-buildkite",
  "version": "1.0.1",
  "description": "A Backstage plugin which integrates with Buildkite pipelines",
  "backstage": {
    "role": "frontend-plugin",
    "pluginId": "buildkite",
    "pluginPackages": [
      "@buildkite/backstage-plugin-buildkite"
    ],
    "features": {
      "./alpha": "@backstage/FrontendPlugin"
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.tsx",
    "./components": "./src/components/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.tsx"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "homepage": "https://buildkite.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildkite/backstage-plugin.git"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "backstage-cli package build",
    "clean": "backstage-cli package clean",
    "lint": "backstage-cli package lint",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "start": "backstage-cli package start",
    "test": "backstage-cli package test",
    "tsc": "tsc"
  },
  "dependencies": {
    "@backstage/catalog-model": "^1.7.5",
    "@backstage/config": "^1.3.5",
    "@backstage/core-compat-api": "^0.5.3",
    "@backstage/core-components": "^0.18.1",
    "@backstage/core-plugin-api": "^1.11.0",
    "@backstage/frontend-defaults": "^0.3.1",
    "@backstage/frontend-plugin-api": "^0.12.0",
    "@backstage/theme": "^0.7.0",
    "@date-io/date-fns": "^3.2.1",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.61",
    "@material-ui/pickers": "^3.3.11",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "react-use": "^17.6.0"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.13.1 || ^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.25.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.34.4",
    "@backstage/core-app-api": "^1.18.0",
    "@backstage/dev-utils": "^1.1.16",
    "@backstage/eslint-plugin": "^0.1.10",
    "@backstage/test-utils": "^1.7.12",
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^18.2.61",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9.33.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-notice": "^1.0.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-testing-library": "^6.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.12.0",
    "msw": "^2.11.5",
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.13.1 || ^17.0.0 || ^18.0.0",
    "react-router-dom": "^6.25.0",
    "typescript-eslint": "^8.39.1"
  }
}
